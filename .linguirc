{
  "catalogs": [
    {
      "path": "locales/{locale}",
      "include": [
        "pages",
        "components",
        "containers"
      ]
    }
  ],
  "format": "po",
  "formatOptions": {
    "origins": true,
    "lineNumbers": false
  },
  "orderBy": "origin",
  "locales": [
    "en",
    "pt",
    "es",
    "fr",
    "de",
    "ko"
  ],
  "sourceLocale": "en",
  "fallbackLocales": {
    "default": "en"
  }
}
